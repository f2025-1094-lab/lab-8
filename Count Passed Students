#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter number of students: ";
    cin >> n;
    int marks[100];
    int passCount = 0;
    cout << "Enter marks: ";
    for (int i = 0; i < n; i++) {
        cin >> marks[i];
        if (marks[i] >= 50) {
            passCount++;
        }
    }
    cout<<"Number of passed students" << passCount << endl;
    return 0;
}
