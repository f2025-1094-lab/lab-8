#include <iostream>
using namespace std;
int main() {
    int arr[7];
    cout<<"Enter 7 elements";
    for (int i = 0; i < 7; i++) {
        cin >> arr[i];
    }
    for (int i = 0; i < 7 / 2; i++) {
        int temp = arr[i];
        arr[i] = arr[7 - 1 - i];
        arr[7 - 1 - i] = temp;
    }
    cout<<"Reversed array";
    for (int i = 0; i < 7; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;
    return 0;
}
